{
  "name": "mithril-bootstrap-modal-example",
  "version": "1.0.0",
  "description": "An example for using github.com/cvermeul/mithril-bootstrap-modal",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "mkdirp css && cpx node_modules/bootstrap/dist/css/bootstrap.min.css css/",
    "build:js": "mkdirp js && browserify -g uglifyify src/app.js -o js/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "mithril": "^0.2.5",
    "mithril-bootstrap-modal": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "cpx": "^1.5.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.1",
    "uglifyify": "^3.0.4"
  }
}
